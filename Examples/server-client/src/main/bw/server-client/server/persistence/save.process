<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="pfx" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="pfx4" xmlns:ns3="cfshp" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://windyroad.org/coffee-shop">
    <xsd:import namespace="http://windyroad.org/coffee-shop" schemaLocation="/server/model/CoffeeShop.xsd"/>
    <pd:name>server/persistence/save.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:coffee-order"/>
    <pd:startX>94</pd:startX>
    <pd:startY>68</pd:startY>
    <pd:returnBindings>
        <xsl:variable name="user" select="$GetVariable/root/user[etag=$Start/etag]"/>
        <xsl:copy-of select="$user"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endX>716</pd:endX>
    <pd:endY>67</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338360464424</pd:targetNamespace>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>535</pd:x>
        <pd:y>73</pd:y>
        <config>
            <variableConfig>/server/persistence/sudo-database.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:variable name="var" select="$Start/pfx:coffee-order/pfx:firstname"/>
                <xsl:if test="count($getAll/root/pfx:coffee-order[pfx:firstname!=$var]) > 0">
                    <pfx:coffee-order>
                        <xsl:copy-of select="$getAll/root/pfx:coffee-order[pfx:firstname!=$var]/ancestor-or-self::*/namespace::node()"/>
                        <xsl:copy-of select="$getAll/root/pfx:coffee-order[pfx:firstname!=$var]/@*"/>
                        <xsl:copy-of select="$getAll/root/pfx:coffee-order[pfx:firstname!=$var]/node()"/>
                    </pfx:coffee-order>
                </xsl:if>
                <pfx:coffee-order>
                    <xsl:copy-of select="$Start/pfx:coffee-order/ancestor-or-self::*/namespace::node()"/>
                    <xsl:copy-of select="$Start/pfx:coffee-order/@*"/>
                    <xsl:copy-of select="$Start/pfx:coffee-order/node()"/>
                </pfx:coffee-order>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="getAll">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>299</pd:x>
        <pd:y>79</pd:y>
        <config>
            <processName>/server/persistence/getAll.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>getAll</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>getAll</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>