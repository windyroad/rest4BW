<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="pfx" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.tibco.com/schemas/server-client/server/shared variables/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/server-client/server/shared variables/Schema.xsd" schemaLocation="/server/shared variables/Schema.xsd"/>
    <pd:name>server/shared variables/saveUser.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:user"/>
    <pd:startX>94</pd:startX>
    <pd:startY>68</pd:startY>
    <pd:returnBindings>
        <xsl:variable name="user" select="$GetVariable/root/user[etag=$Start/etag]"/>
        <xsl:copy-of select="$user"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endX>716</pd:endX>
    <pd:endY>67</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338360464424</pd:targetNamespace>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>535</pd:x>
        <pd:y>73</pd:y>
        <config>
            <variableConfig>/server/shared variables/sudo-database.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:copy-of select="$Start/pfx:user"/>
                <xsl:copy-of select="$GetVariable/root/pfx:user[pfx:etag!=$Start/pfx:user/pfx:etag]"/>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GetVariable">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>300</pd:x>
        <pd:y>71</pd:y>
        <config>
            <variableConfig>/server/shared variables/sudo-database.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetVariable</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>