                    </replace>
                </param>
                <param>
                    <pattern>\{amount\}</pattern>
                    <replace>
                        <xsl:value-of select="$de-multiplex/pfx3:user/pfx3:amount"/>
                    </replace>
                </param>
                <param>
                    <pattern>\{status\}</pattern>
                    <replace>
                        <xsl:value-of select="$de-multiplex/pfx3:user/pfx3:status"/>
                    </replace>
                </param>
                <param>
                    <pattern>\{numberOfUsers\}</pattern>
                    <replace>
                        <xsl:value-of select="count($admin-page---get-users/root/pfx3:user)"/>
                    </replace>
                </param>
            </replace>
