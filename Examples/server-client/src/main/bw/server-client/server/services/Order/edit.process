    </pd:activity>
    <pd:activity name="406 Not Acceptable">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>378</pd:x>
        <pd:y>216</pd:y>
        <config>
            <processName>/REST/StatusCodes/4xx Client Error/406 Not Acceptable.process</processName>
        </config>
