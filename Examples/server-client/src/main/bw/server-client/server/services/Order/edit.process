                    <xsd:element ref="pfx:request"/>
                    <xsd:element name="firstname" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>97</pd:startX>
    <pd:startY>107</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$OK/pfx:response"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:choice>
            <xsd:element ref="pfx:response"/>
            <xsd:element ref="pfx:error"/>
        </xsd:choice>
    </pd:endType>
    <pd:endX>725</pd:endX>
    <pd:endY>107</pd:endY>
    <pd:errorSchemas>
        <HTTP-Error ref="pfx:error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338181537186</pd:targetNamespace>
    <pd:activity name="200 OK">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>584</pd:x>
        <pd:y>109</pd:y>
        <config>
            <processName>/REST/StatusCodes/2xx Successful/200 OK.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:response/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="transition state">
