            <root>
                <firstname>
                    <xsl:value-of select="$Start/root/firstname"/>
                </firstname>
                <transition-action-name>
                    <xsl:value-of select="$Start/root/pfx:request/pfx:post/pfx:part/pfx:meta[@name='transition-action-name']/@value"/>
                </transition-action-name>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>226</pd:x>
        <pd:y>107</pd:y>
        <config/>
