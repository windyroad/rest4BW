<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://windyroad.org/coffee-shop" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>test/uatTestSuite/fixtures/validateStateTransitionFixture.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="csv" type="xsd:string"/>
    </pd:startType>
    <pd:startX>91</pd:startX>
    <pd:startY>85</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>727</pd:endX>
    <pd:endY>85</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338665230923</pd:targetNamespace>
    <pd:activity name="buildOrder">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>457</pd:x>
        <pd:y>85</pd:y>
        <config>
            <processName>/test/uatTestSuite/util/buildOrder.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <initial-state>
                    <xsl:value-of select="$tokenize/root/output[1]"/>
                </initial-state>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="tokenize">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>222</pd:x>
        <pd:y>85</pd:y>
        <config>
            <processName>/REST/Private/Common/string/tokenize.process</processName>
        </config>
        <pd:inputBindings>
            <tokenize>
                <string>
                    <xsl:value-of select="$Start/csv"/>
                </string>
                <pattern>
                    <xsl:value-of select="'\|'"/>
                </pattern>
            </tokenize>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="validateStateTransition">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>601</pd:x>
        <pd:y>85</pd:y>
        <config>
            <processName>/test/uatTestSuite/scenarioOutlines/validateStateTransition.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:copy-of select="$buildOrder/ns:coffee-order"/>
                <transition-action>
                    <xsl:value-of select="$tokenize/root/output[2]"/>
                </transition-action>
                <expected-end-state>
                    <xsl:value-of select="$tokenize/root/output[3]"/>
                </expected-end-state>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>tokenize</pd:from>
        <pd:to>buildOrder</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>tokenize</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>validateStateTransition</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>buildOrder</pd:from>
        <pd:to>validateStateTransition</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>