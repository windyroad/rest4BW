    <pd:activity name="build coffee-shop options">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>379</pd:x>
        <pd:y>197</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx6:options"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <xsl:variable name="type" select="'application/vnd.coffee-shop+xml'"/>
            <xsl:variable name="url" select="'/coffee-shop/order'"/>
            <root>
                <pfx6:options>
                    <pfx6:link>
                        <xsl:attribute name="title">Create Order</xsl:attribute>
                        <xsl:attribute name="rel">create</xsl:attribute>
                        <xsl:attribute name="type">
                            <xsl:value-of select="$type"/>
                        </xsl:attribute>
                        <xsl:attribute name="href">
                            <xsl:value-of select="$url"/>
                        </xsl:attribute>
                        <xsl:attribute name="method">POST</xsl:attribute>
                        <pfx6:parameter>
                            <xsl:attribute name="title">Firstname</xsl:attribute>
                            <xsl:attribute name="name">firstname</xsl:attribute>
                            <xsl:attribute name="type">text</xsl:attribute>
                        </pfx6:parameter>
                        <pfx6:parameter>
                            <xsl:attribute name="title">Coffee</xsl:attribute>
                            <xsl:attribute name="name">coffee</xsl:attribute>
                            <xsl:attribute name="type">text</xsl:attribute>
                        </pfx6:parameter>
                        <pfx6:parameter>
                            <xsl:attribute name="title">Sugar</xsl:attribute>
                            <xsl:attribute name="name">sugar</xsl:attribute>
                            <xsl:attribute name="type">text</xsl:attribute>
                        </pfx6:parameter>
                    </pfx6:link>
                    <pfx6:link>
                        <xsl:attribute name="title">List Orders</xsl:attribute>
                        <xsl:attribute name="rel">list</xsl:attribute>
                        <xsl:attribute name="type">
                            <xsl:value-of select="$type"/>
                        </xsl:attribute>
                        <xsl:attribute name="href">
                            <xsl:value-of select="$url"/>
                        </xsl:attribute>
                        <xsl:attribute name="method">GET</xsl:attribute>
                    </pfx6:link>
                </pfx6:options>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="build order options">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>436</pd:x>
        <pd:y>74</pd:y>
        <config>
            <processName>/server/services/Order/Private/build options for order.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <firstname>
                    <xsl:value-of select="$get/pfx4:coffee-order/pfx4:firstname"/>
                </firstname>
                <state>
                    <xsl:value-of select="$get/pfx4:coffee-order/pfx4:state"/>
                </state>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="de-multiplex">
