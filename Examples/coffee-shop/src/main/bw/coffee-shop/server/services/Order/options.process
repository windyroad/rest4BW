                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx6:options"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:choose>
                    <xsl:when test="$build-coffee-shop-options/root/pfx6:options">
                        <xsl:copy-of select="$build-coffee-shop-options/root/pfx6:options"/>
                    </xsl:when>
                    <xsl:otherwise>
                        <xsl:copy-of select="$build-order-options/root/pfx6:options"/>
                    </xsl:otherwise>
                </xsl:choose>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="get">
