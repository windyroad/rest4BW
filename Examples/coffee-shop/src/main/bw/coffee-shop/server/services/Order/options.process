<pd:ProcessDefinition xmlns:ns4="http://windyroad.org/bwunit-abstract" xmlns:pfx2="http://windyroad.org/rest-delegate.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns7="http://windyroad.org/wr-http.xsd" xmlns:pfx3="http://www.tibco.com/schemas/coffee-shop/server/shared variables/Schema.xsd" xmlns:pfx="http://windyroad.org/rest4BW.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx1="http://windyroad.org/bwunit-abstract/installLicense2Impl" xmlns:ns5="http://windyroad.org/HTTP1.1-StatusCodeDefinitions" xmlns:pfx4="http://windyroad.org/coffee-shop" xmlns:ns6="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns31="http://windyroad.org/bwunit-asserts-0.0.0.xsd" xmlns:pfx21="http://windyroad.org/bwunit-concrete" xmlns:ns11="http://windyroad.org/bwunit-0.0.0.xsd" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx5="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx6="http://windyroad.org/coffee-shop-interface" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:ns3="http://xmlns.tibco.com/encodings/mime" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
