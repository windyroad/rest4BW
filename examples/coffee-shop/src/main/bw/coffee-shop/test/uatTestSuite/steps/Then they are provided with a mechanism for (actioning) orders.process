<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://windyroad.org/coffee-shop-interface" xmlns:ns1="http://windyroad.org/coffee-shop" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://windyroad.org/bwunit-asserts-0.0.0.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://windyroad.org/wr-http.xsd">
    <xsd:import namespace="http://windyroad.org/coffee-shop" schemaLocation="/coffee-shop/model/CoffeeShop.xsd"/>
    <xsd:import namespace="http://windyroad.org/wr-http.xsd" schemaLocation="/rest4BW/Private/Schemas/HttpConnection.xsd"/>
    <xsd:import namespace="http://windyroad.org/coffee-shop-interface" schemaLocation="/coffee-shop/interface/Interface.xsd"/>
    <pd:name>test/uatTestSuite/steps/Then they are provided with a mechanism for (actioning) orders.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="expected">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="actions" type="xsd:string" maxOccurs="unbounded"/>
                                <xsd:element name="firstname" type="xsd:string" minOccurs="0"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="actual">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element ref="ns2:options"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>104</pd:startX>
    <pd:startY>110</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>707</pd:endX>
    <pd:endY>110</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338635071359</pd:targetNamespace>
    <pd:activity name="all order options">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>248</pd:x>
        <pd:y>110</pd:y>
        <config>
            <processName>/coffee-shop/services/Order/Private/all order options.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <firstname>
                    <xsl:value-of select="$Start/root/expected/firstname"/>
                </firstname>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="AssertEqual">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>534</pd:x>
        <pd:y>114</pd:y>
        <config>
            <processName>/BWUnit/Public/Asserts/AssertEqual.process</processName>
        </config>
        <pd:inputBindings>
            <ns:equal-input>
                <ns:expected>
                    <ns2:options>
                        <xsl:copy-of select="$all-order-options/root/ns2:options/ns2:link[@rel=$Start/root/expected/actions]"/>
                    </ns2:options>
                </ns:expected>
                <ns:actual>
                    <xsl:copy-of select="$Start/root/actual/ns2:options"/>
                </ns:actual>
            </ns:equal-input>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>all order options</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>AssertEqual</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>all order options</pd:from>
        <pd:to>AssertEqual</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>