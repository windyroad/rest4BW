<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://windyroad.org/wr-http.xsd" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://windyroad.org/wr-http.xsd" schemaLocation="/REST/Public/Schemas/Http.xsd"/>
    <pd:name>Test/UnitTests/RequestResponseFixture.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="http-request-xml-file-location" type="xsd:string"/>
                    <xsd:element ref="ns:respHeaders"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>91</pd:startX>
    <pd:startY>62</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>930</pd:endX>
    <pd:endY>68</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <http>
            <xsd:element name="request" type="xsd:string"/>
        </http>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1334630190234</pd:targetNamespace>
    <pd:activity name="REST Interface">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:description>When I invoke the REST interface</pd:description>
        <pd:x>608</pd:x>
        <pd:y>69</pd:y>
        <config>
            <processName>/REST/Public/Process/REST Interface.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Parse-Request/ns:request"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="QuickAssert">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>788</pd:x>
        <pd:y>66</pd:y>
        <config>
            <processName>/Test/UnitTests/QuickAssert.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <actual>
                    <xsl:value-of select="$REST-Interface/ns:response/ns:respHeaders/ns:StatusLine"/>
                </actual>
                <expected>
                    <xsl:value-of select="$Start/root/ns:respHeaders/ns:StatusLine"/>
                </expected>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Read File">
        <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
        <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
        <pd:x>296</pd:x>
        <pd:y>68</pd:y>
        <config>
            <encoding>text</encoding>
        </config>
        <pd:inputBindings>
            <ns2:ReadActivityInputClass>
                <fileName>
                    <xsl:value-of select="concat($_globalVariables/ns3:GlobalVariables/RESTLib/project-home,'/',$Start/root/http-request-xml-file-location)"/>
                </fileName>
            </ns2:ReadActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse Request">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:description>Given a HTTP request for static data</pd:description>
        <pd:x>447</pd:x>
        <pd:y>67</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="ns:request"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Read-File/ns1:ReadActivityOutputTextClass/fileContent/textContent"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>REST Interface</pd:from>
        <pd:to>QuickAssert</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>QuickAssert</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Read File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Read File</pd:from>
        <pd:to>Parse Request</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse Request</pd:from>
        <pd:to>REST Interface</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>