<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx2="http://windyroad.org/rest-delegate.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pfx="http://windyroad.org/rest4BW.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://windyroad.org/HTTP1.1-StatusCodeDefinitions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
    <xsd:import namespace="http://windyroad.org/rest-delegate.xsd"/>
    <xsd:import namespace="http://windyroad.org/HTTP1.1-StatusCodeDefinitions" schemaLocation="/rest4BW/Private/Schemas/StatusCodeDefinitions.xsd"/>
    <xsd:import namespace="http://windyroad.org/rest4BW.xsd" schemaLocation="/rest4BW/Schemas/rest4BW.xsd"/>
    <pd:name>rest4BW/RSS/REST Delegate.process</pd:name>
    <pd:description/>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:request"/>
    <pd:startX>67</pd:startX>
    <pd:startY>78</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$feed/pfx:response"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:response"/>
    <pd:endX>481</pd:endX>
    <pd:endY>80</pd:endY>
    <pd:errorSchemas>
        <Error ref="pfx:error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338181537186</pd:targetNamespace>
    <pd:activity name="feed">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>369</pd:x>
        <pd:y>73</pd:y>
        <config>
            <processName>/rest4BW/RSS/feed.process</processName>
            <processNameXPath/>
        </config>
        <pd:inputBindings>
            <input>
                <xsl:copy-of select="$Start/pfx:request"/>
                <xsl:copy-of select="$Get-Mappings/mappings/feed[path=$Start/pfx:request/pfx:command][1]"/>
            </input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Mappings">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>171</pd:x>
        <pd:y>77</pd:y>
        <config>
            <variableConfig>/rest4BW/RSS/Feed Mapper.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="404 Not found">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>367</pd:x>
        <pd:y>192</pd:y>
        <config>
            <faultName>Error</faultName>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <message>Not Found</message>
                <messageCode>
                    <xsl:value-of select="404"/>
                </messageCode>
                <data>
                    <pfx:error xsi:type="ns:sc-404_Not_Found">
                        <pfx:text-content>Not Found</pfx:text-content>
                        <pfx:headers/>
                    </pfx:error>
                </data>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>feed</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get Mappings</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Mappings</pd:from>
        <pd:to>feed</pd:to>
        <pd:xpathDescription>Feed at Path?</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($Get-Mappings/mappings/feed[path=$Start/pfx:request/pfx:command])</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Mappings</pd:from>
        <pd:to>404 Not found</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>