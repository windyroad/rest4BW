<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pfx="http://windyroad.org/rest4BW.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://windyroad.org/HTTP1.1-StatusCodeDefinitions" xmlns:ns2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
    <xsd:import namespace="http://windyroad.org/rest4BW.xsd" schemaLocation="/REST/Private/Schemas/rest4BW.xsd"/>
    <pd:name>test/quickTestSuite/Dynamic Process Overrides/Throw HTTP exception.process</pd:name>
    <pd:description>N.B. Throws an exception if invoked!</pd:description>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:request"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx:response xsi:type="ns:sc-500_Internal_Server_Error"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:choice>
            <xsd:element ref="pfx:response"/>
            <xsd:element ref="pfx:error"/>
        </xsd:choice>
    </pd:endType>
    <pd:endX>534</pd:endX>
    <pd:endY>62</pd:endY>
    <pd:errorSchemas>
        <HTTP-Error ref="pfx:error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338181537186</pd:targetNamespace>
    <pd:activity name="405 Method Not Allowed">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>345</pd:x>
        <pd:y>202</pd:y>
        <config>
            <faultName>HTTP-Error</faultName>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <data>
                    <pfx:error xsi:type="ns:sc-405_Method_Not_Allowed">
                        <pfx:headers>
                            <xsl:attribute name="allow">
                                <xsl:value-of select="'these, methods, are, allowed, for the requested resource'"/>
                            </xsl:attribute>
                        </pfx:headers>
                    </pfx:error>
                </data>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>405 Method Not Allowed</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>false()</pd:xpath>
    </pd:transition>
</pd:ProcessDefinition>