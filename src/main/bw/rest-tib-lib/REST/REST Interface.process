<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.tibco.com/pe/EngineTypes" xmlns:pfx3="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx2="http://windyroad.org/wr-http.xsd" xmlns:pfx="http://windyroad.org/rest-tib-lib.xsd">
    <xsd:import namespace="http://windyroad.org/wr-http.xsd" schemaLocation="/REST/Private/Schemas/HttpConnection.xsd"/>
    <xsd:import namespace="http://windyroad.org/rest-tib-lib.xsd" schemaLocation="/REST/Private/Schemas/rest-tib-lib.xsd"/>
    <pd:name>REST/REST Interface.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx2:request"/>
                    <xsd:element name="dynamicDelegatorProcessOverride" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>78</pd:startX>
    <pd:startY>91</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="not(exists($Render-HTTP-error-response))">
                <xsl:copy-of select="$Render-HTTP-response/pfx2:response"/>
            </xsl:when>
            <xsl:otherwise>
                <xsl:copy-of select="$Render-HTTP-error-response/pfx2:response"/>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:response"/>
    <pd:endX>788</pd:endX>
    <pd:endY>86</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1334627030682</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>169</pd:x>
        <pd:y>2</pd:y>
        <pd:width>516</pd:width>
        <pd:height>155</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>simpleGroup</pd:groupType>
            <pd:serializable>false</pd:serializable>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Parse HTTP request</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Render HTTP response</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Dynamic Delegator</pd:from>
            <pd:to>Render HTTP response</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Parse HTTP request</pd:from>
            <pd:to>Dynamic Delegator</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Dynamic Delegator">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:description>N.B. This must be configured via the Global Variable!</pd:description>
            <pd:x>429</pd:x>
            <pd:y>87</pd:y>
            <config>
                <processNameXPath>$Start/root/dynamicDelegatorProcessOverride</processNameXPath>
                <processName>/REST/Example/server/Default Delegate.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Parse-HTTP-request/pfx:request"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Parse HTTP request">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>275</pd:x>
            <pd:y>87</pd:y>
            <config>
                <processName>/REST/Private/Process/REST Interface/parseHTTPRequest.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Start/root/pfx2:request"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Render HTTP response">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>584</pd:x>
            <pd:y>94</pd:y>
            <config>
                <processName>/REST/Private/Process/REST Interface/renderHTTPResponse.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Dynamic-Delegator/pfx:response"/>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Internal Server Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>476</pd:x>
        <pd:y>315</pd:y>
        <config>
            <processName>/REST/StatusCodes/5xx Server Error/500 Internal Server Error.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <errorMessage>
                    <xsl:value-of select="if (not($_globalVariables/pfx3:GlobalVariables/REST/exception-shielding = 'true'))&#xA;then concat('PID:',$_processContext/pfx4:ProcessContext/ProcessId,' - ',$_error/pfx4:ErrorReport/Msg)&#xA;else concat('PID:',$_processContext/pfx4:ProcessContext/ProcessId)"/>
                </errorMessage>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>380</pd:x>
        <pd:y>210</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Render HTTP error response">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>602</pd:x>
        <pd:y>217</pd:y>
        <config>
            <processName>/REST/Private/Process/REST Interface/renderHTTPResponse.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="$_error_Internal-Server-Error">
                    <xsl:copy-of select="$_error_Internal-Server-Error/pfx:error"/>
                </xsl:when>
                <xsl:otherwise>
                    <xsl:copy-of select="$_error/pfx4:ErrorReport/Data/pfx:error"/>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write to log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>229</pd:x>
        <pd:y>211</pd:y>
        <config>
            <processName>/REST/Private/Common/logging/writeToLog.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_error/pfx4:ErrorReport/Msg"/>
                </message>
                <xsl:if test="$_error/pfx4:ErrorReport/MsgCode">
                    <messageCode>
                        <xsl:value-of select="$_error/pfx4:ErrorReport/MsgCode"/>
                    </messageCode>
                </xsl:if>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Internal Server Error</pd:from>
        <pd:to>Render HTTP error response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Internal Server Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Render HTTP error response</pd:to>
        <pd:xpathDescription>http error?</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($_error/pfx4:ErrorReport/Data/pfx:error)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Write to log</pd:from>
        <pd:to>Null</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Write to log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render HTTP error response</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>