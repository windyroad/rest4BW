<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx2="http://windyroad.org/wr-http.xsd" xmlns:pfx3="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx="http://windyroad.org/rest-tib-lib.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
    <xsd:import namespace="http://windyroad.org/wr-http.xsd" schemaLocation="/REST/Private/Schemas/HttpConnection.xsd"/>
    <xsd:import namespace="http://windyroad.org/rest-tib-lib.xsd" schemaLocation="/REST/Private/Schemas/rest-tib-lib.xsd"/>
    <pd:name>REST/Public/Server/REST Interface.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx2:request"/>
                    <xsd:element name="dynamicDelegatorProcessOverride" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>79</pd:startX>
    <pd:startY>67</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="not(exists($Map-Exception-to-HTTP-Response))">
                <xsl:copy-of select="$Map-REST-response-to-HTTP-response/pfx2:response"/>
            </xsl:when>
            <xsl:otherwise>
                <xsl:copy-of select="$Map-Exception-to-HTTP-Response/pfx2:response"/>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:response"/>
    <pd:endX>803</pd:endX>
    <pd:endY>71</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1334627030682</pd:targetNamespace>
    <pd:activity name="Internal Server Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>398</pd:x>
        <pd:y>388</pd:y>
        <config>
            <processName>/REST/Public/Server/StatusCodes/500 Internal Server Error.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <errorMessage>
                    <xsl:value-of select="$_error/pfx4:ErrorReport/Msg"/>
                </errorMessage>
                <generateError>
                    <xsl:value-of select="false()"/>
                </generateError>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Exception to HTTP Response">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>698</pd:x>
        <pd:y>389</pd:y>
        <config>
            <processName>/REST/Private/Process/Mappers/Map Exception to HTTP Response.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="$Internal-Server-Error">
                    <xsl:copy-of select="$Internal-Server-Error/pfx:error"/>
                </xsl:when>
                <xsl:otherwise>
                    <xsl:copy-of select="$_error/pfx4:ErrorReport/Data/pfx:error"/>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>169</pd:x>
        <pd:y>2</pd:y>
        <pd:width>516</pd:width>
        <pd:height>155</pd:height>
        <pd:collapsedWidth>52</pd:collapsedWidth>
        <pd:collapsedHeight>69</pd:collapsedHeight>
        <pd:groupVariables/>
        <config>
            <pd:groupType>simpleGroup</pd:groupType>
            <pd:serializable>false</pd:serializable>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Map HTTP request to REST request</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Map REST response to HTTP response</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Dynamic Delegator</pd:from>
            <pd:to>Map REST response to HTTP response</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Map HTTP request to REST request</pd:from>
            <pd:to>Dynamic Delegator</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Dynamic Delegator">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:description>N.B. This must be configured via the Global Variable!</pd:description>
            <pd:x>418</pd:x>
            <pd:y>67</pd:y>
            <config>
                <processName>/REST/Private/Process/REST Interface/Delegator Placeholder.process</processName>
                <processNameXPath>$Start/root/dynamicDelegatorProcessOverride</processNameXPath>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Map-HTTP-request-to-REST-request/pfx:request"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Map HTTP request to REST request">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>247</pd:x>
            <pd:y>74</pd:y>
            <config>
                <processName>/REST/Private/Process/Mappers/Map HTTP request to REST request.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Start/root/pfx2:request"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Map REST response to HTTP response">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>581</pd:x>
            <pd:y>74</pd:y>
            <config>
                <processName>/REST/Private/Process/Mappers/Map REST response to HTTP response.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Dynamic-Delegator/pfx:response"/>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>415</pd:x>
        <pd:y>280</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Map Exception to HTTP Response</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Internal Server Error</pd:from>
        <pd:to>Map Exception to HTTP Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Null</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Internal Server Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Map Exception to HTTP Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($_error/pfx4:ErrorReport/Data/pfx:error)</pd:xpath>
    </pd:transition>
</pd:ProcessDefinition>