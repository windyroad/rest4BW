<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://windyroad.org/rest-tib-lib.xsd">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://windyroad.org/rest-tib-lib.xsd" schemaLocation="/REST/Private/Schemas/rest-tib-lib.xsd"/>
    <pd:name>quickTestSuite/Dynamic Process Overrides/Throw HTTP exception.process</pd:name>
    <pd:description>N.B. Throws an exception if invoked!</pd:description>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:request"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx:response/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:response"/>
    <pd:endX>534</pd:endX>
    <pd:endY>62</pd:endY>
    <pd:errorSchemas>
        <HTTP-Error ref="pfx:error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1338181537186</pd:targetNamespace>
    <pd:activity name="500 Internal Server Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>336</pd:x>
        <pd:y>73</pd:y>
        <config>
            <processName>/REST/Public/Server/StatusCodes/500 Internal Server Error.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <errorMessage>
                    <xsl:value-of select="'The &quot;Delegator&quot; Dynamic Process Override was not invoked!'"/>
                </errorMessage>
                <generateError>
                    <xsl:value-of select="true()"/>
                </generateError>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>500 Internal Server Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>500 Internal Server Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>