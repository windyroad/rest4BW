<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://windyroad.org/RESTlib.xsd" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://windyroad.org/bwunit-asserts-0.0.0.xsd">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://windyroad.org/RESTlib.xsd" schemaLocation="/REST/Public/Schemas/RESTlib.xsd"/>
    <pd:name>RESTTestSuite/Feature 2 - Multipart Form Data parser TestCase/Scenario 3 - Single Parameter with value.process</pd:name>
    <pd:description>@Timeout(60000)</pd:description>
    <pd:startName>Start</pd:startName>
    <pd:startX>150</pd:startX>
    <pd:startY>63</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>605</pd:endX>
    <pd:endY>68</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1301865816031</pd:targetNamespace>
    <pd:activity name="AssertEqual">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:description>Then the returned set has an entry with the name "somefield" and the value:
"""value"""</pd:description>
        <pd:x>478</pd:x>
        <pd:y>65</pd:y>
        <config>
            <processName>/BWUnit/Public/Asserts/AssertEqual.process</processName>
        </config>
        <pd:inputBindings>
            <ns:equal-input>
                <ns:expected>
                    <ns1:post>
                        <ns1:part>
                            <ns1:meta>
                                <xsl:attribute name="name">Content-Disposition</xsl:attribute>
                                <xsl:attribute name="value">form-data</xsl:attribute>
                            </ns1:meta>
                            <ns1:meta>
                                <xsl:attribute name="name">name</xsl:attribute>
                                <xsl:attribute name="value">somefield</xsl:attribute>
                            </ns1:meta>
                            <ns1:meta>
                                <xsl:attribute name="name">Content-Type</xsl:attribute>
                                <xsl:attribute name="value">text/plain</xsl:attribute>
                            </ns1:meta>
                            <ns1:content>value</ns1:content>
                        </ns1:part>
                    </ns1:post>
                </ns:expected>
                <ns:actual>
                    <xsl:copy-of select="$parsePost/ns1:post"/>
                </ns:actual>
            </ns:equal-input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="parsePost">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:description>When the content type "multipart/form-data; boundary=----WebKitFormBoundaryBqB4t3RT1FgEAv7v" is parsed with the following post data:
"""------WebKitFormBoundaryBqB4t3RT1FgEAv7v
Content-Disposition: form-data; name="somefield"
Content-Type: text/plain

value

------WebKitFormBoundaryBqB4t3RT1FgEAv7v--"""</pd:description>
        <pd:x>315</pd:x>
        <pd:y>66</pd:y>
        <config>
            <processName>/REST/Public/Process/Http/parsePost.process</processName>
        </config>
        <pd:inputBindings>
            <input>
                <post>------WebKitFormBoundaryBqB4t3RT1FgEAv7v
Content-Disposition: form-data; name="somefield"
Content-Type: text/plain

value

------WebKitFormBoundaryBqB4t3RT1FgEAv7v--</post>
                <contentType>multipart/form-data; boundary=----WebKitFormBoundaryBqB4t3RT1FgEAv7v</contentType>
            </input>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>AssertEqual</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>parsePost</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>parsePost</pd:from>
        <pd:to>AssertEqual</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
